{
  "name": "whereami",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && webpack --progress -p --config webpack.production.config.js",
    "start": "webpack-dev-server --progress --history-api-fallback --hot --inline --port 9000 --open",
    "dev": "webpack-dev-server --progress --history-api-fallback --hot --inline --host 0.0.0.0 --port 9000 --open",
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "eslint ./app",
    "test": "npm run lint && ava",
    "createServer": "cd dist && npm init -f && npm i express -S"
  },
  "author": "IAG Limited",
  "license": "ISC",
  "dependencies": {
    "axios": "0.17.1",
    "bootstrap": "4.0.0",
    "chart.js": "2.7.1",
    "classnames": "2.2.5",
    "crypto-js": "3.1.9-1",
    "date-fns": "1.29.0",
    "express": "4.16.2",
    "firebase": "^4.11.0",
    "firebaseui": "^2.6.3",
    "flexboxgrid": "6.3.1",
    "jquery": "3.3.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.4",
    "moment": "2.19.3",
    "normalize.css": "7.0.0",
    "numeral": "2.0.6",
    "pikaday": "1.6.1",
    "popper.js": "1.12.9",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-feather": "1.0.8",
    "react-ga": "2.3.5",
    "react-google-autocomplete": "1.0.15",
    "react-google-maps": "^9.4.5",
    "react-jsonschema-form": "1.0.0",
    "react-router-dom": "4.2.2",
    "react-runkit": "0.9.0",
    "react-stripe-elements": "1.4.1",
    "react-tippy": "1.2.2",
    "react-toastify": "^3.3.5"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.2.1",
    "css-loader": "0.28.7",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.7.2",
    "postcss": "6.0.14",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.0.8",
    "postcss-nesting": "4.2.1",
    "rimraf": "2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ],
        "features": {
          "customProperties": {
            "preserve": true
          }
        }
      },
      "postcss-nesting": {}
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "indent": [
        0,
        2
      ],
      "semi": [
        "error",
        "never"
      ],
      "arrow-parens": 0,
      "linebreak-style": 0,
      "object-curly-newline": 0,
      "comma-dangle": 0,
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "react/require-default-props": 0,
      "max-len": [
        1,
        150,
        4,
        {
          "ignorePattern": "\\s*<"
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": [
        "error",
        {
          "caseSensitive": false
        }
      ],
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/require-extension": 0,
      "class-methods-use-this": 0,
      "no-console": 0,
      "react/no-string-refs": 1,
      "no-unused-vars": [
        2,
        {
          "varsIgnorePattern": "[iI]gnored"
        }
      ],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "react/forbid-prop-types": 0,
      "jsx-a11y/img-has-alt": 0,
      "jsx-a11y/label-has-for": [
        2,
        {
          "required": {
            "every": [
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ]
    }
  }
}
